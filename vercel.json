{
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXTAUTH_URL": {
      "development": "http://localhost:3000",
      "production": "https://your-production-url.vercel.app"
    },
    "NEXTAUTH_SECRET": "@nextauth-secret"
  }
}
